<Project>

  <Import Project="./common.props" />


  <!-- Target frameworks  -->
  <PropertyGroup Condition=" '$(OS)' == 'Windows_NT' ">
      <TargetFrameworks>netstandard1.3;netstandard2.0;net452;net462;net472</TargetFrameworks>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(OS)' != 'Windows_NT' ">
      <TargetFrameworks>netstandard1.3;netstandard2.0;</TargetFrameworks>
  </PropertyGroup>


  <!-- NuGet package attributes  -->
  <PropertyGroup>
    <PackageTags>DDD NETCore</PackageTags>
    <PackageReleaseNotes>v2.2 adds support for updating entities.</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/jburnett/J3DI</PackageProjectUrl>
    <PackageLicenseUrl>http://www.apache.org/licenses/LICENSE-2.0</PackageLicenseUrl>
  </PropertyGroup>

</Project>