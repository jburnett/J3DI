<Project>

  <Import Project="./common.props" />


  <!-- Target frameworks  -->
  <PropertyGroup Condition=" '$(OS)' == 'Windows_NT' ">
      <TargetFrameworks>netstandard2.0;net462;net472</TargetFrameworks>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(OS)' != 'Windows_NT' ">
      <TargetFrameworks>netstandard2.0</TargetFrameworks>
  </PropertyGroup>


  <!-- NuGet package attributes  -->
  <PropertyGroup>
    <PackageTags>Domain-Driven-Design DDD DotNet .NET DotNetCore</PackageTags>
    <PackageReleaseNotes>v2.3.0 removes support for Net Standard 1.x ahead of July EOL.</PackageReleaseNotes>
    <PackageProjectUrl>https://bitbucket.org/jburnett/J3DI</PackageProjectUrl>
    <PackageLicense>http://www.apache.org/licenses/LICENSE-2.0</PackageLicense>
  </PropertyGroup>

</Project>